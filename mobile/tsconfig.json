{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    "*/*.jsx",
    "**/*/*.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/tabs/Home.jsx"
  ]
}